<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ndkproject">

    <permission
        android:name="com.example.ndkproject.MY_PERSSION"
        android:protectionLevel="signature" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PrivateActivity"
            android:exported="false" />
        <activity
            android:name=".PublicActivity"
            android:exported="true"
            android:label="@string/title_activity_public"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PartnerActivity"
            android:exported="true" />
        <activity android:name=".PartneruserAcitvity" />
        <activity
            android:name=".InhouseActivity"
            android:exported="true"
            android:permission="com.example.ndkproject.MY_PERSSION" />

        <receiver
            android:name=".PrivateBrocast"
            android:exported="true" />
    </application>

</manifest>